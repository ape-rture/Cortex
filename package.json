{
  "name": "cortex",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node scripts/validate-routing-config.mjs",
    "gm": "node --import tsx src/cli/gm.ts",
    "test:unit": "node --import tsx --test src/**/*.test.ts",
    "digest": "node --import tsx src/cli/digest.ts",
    "prep": "node --import tsx src/cli/prep.ts",
    "content": "node --import tsx src/cli/content.ts",
    "capture": "node --import tsx src/cli/capture.ts",
    "mail": "node --import tsx src/cli/mail.ts",
    "project": "node --import tsx src/cli/project.ts",
    "scrape": "node --import tsx src/cli/scrape.ts",
    "orchestrate": "node --import tsx src/cli/orchestrate.ts",
    "daemon": "node --import tsx src/cli/daemon.ts",
    "slack": "node --import tsx src/cli/slack.ts",
    "telegram": "node --import tsx src/cli/telegram.ts",
    "ralph": "node --import tsx src/cli/ralph.ts",
    "dev:ui": "node --import tsx src/ui/server.ts",
    "dev:dashboard": "vite --config src/ui/dashboard/vite.config.ts",
    "build:dashboard": "vite build --config src/ui/dashboard/vite.config.ts"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.37",
    "@anthropic-ai/sdk": "^0.72.1",
    "@hono/node-server": "^1.14.0",
    "@mozilla/readability": "^0.6.0",
    "@preact/signals": "^2.7.1",
    "@slack/bolt": "^4.6.0",
    "cheerio": "^1.2.0",
    "dotenv": "^17.2.3",
    "googleapis": "^171.1.0",
    "hono": "^4.6.12",
    "linkedom": "^0.18.12",
    "node-cron": "^4.2.1",
    "node-pty": "^1.1.0",
    "openai": "^6.17.0",
    "playwright-core": "^1.58.1",
    "preact": "^10.28.3",
    "robots-parser": "^3.0.1",
    "telegraf": "^4.16.3",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.3",
    "@types/node": "^25.2.0",
    "@types/ws": "^8.18.1",
    "@xterm/addon-canvas": "^0.7.0",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-webgl": "^0.19.0",
    "@xterm/xterm": "^6.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
